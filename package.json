{
  "name": "webprometrics---agency-reporting",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://reports.corporatedigitalmarketing.agency",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "migrate:make": "prisma migrate dev --name init",
    "migrate:create": "node prisma/migrate.js create",
    "migrate:deploy": "node prisma/migrate.js deploy",
    "migrate:rollback": "node prisma/migrate.js rollback",
    "migrate:status": "node prisma/migrate.js status",
    "migrate:list": "node prisma/migrate.js list",
    "migrate:verify": "node prisma/migrate.js verify",
    "start": "node server.js",
    "smoke": "node tests/smoke.mjs",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest -i",
    "test:unit": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest tests/unit -i",
    "test:integration": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest tests/integration -i",
    "test:security": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest tests/security -i",
    "test:coverage": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --coverage -i",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security",
    "start:prod": "node server.js"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^8.1.3",
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.21.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.14.1",
    "googleapis": "^135.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.554.0",
    "qrcode": "^1.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.4.1",
    "speakeasy": "^2.0.0",
    "stripe": "^14.21.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.14.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prisma": "^5.19.1",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.4.1"
  }
